<h2>{% trans %}Attended teams{% endtrans %}</h2>

<table class="table">
  <thead>
    <tr>
      <th>{% trans %}Team{% endtrans %}</th>
      <th>{% trans %}Date{% endtrans %}</th>
      <th>{% trans %}Time{% endtrans %}</th>
    </tr>
  </thead>
  <tfoot>
    <tr class="bg">
      <td colspan="3">&#160;</td>
    </tr>
  </tfoot>
  <tbody>
  {% for schedule in schedules %}
    <tr>
      <td>{{ schedule.teamCategory.teamName }}</td>
      <td>{{ schedule.firstDate|club_date }}</td>
      <td>{{ schedule.firstDate|club_time }} - {{ schedule.endDate|club_time }}</td>
    </tr>
  {% else %}
  <tr>
    <td colspan="3">{% trans %}User has not attended any teams.{% endtrans %}</td>
  </tr>
  {% endfor %}
  </tbody>
</table>


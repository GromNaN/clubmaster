<h2>Ticket</h2>

{% set tickets_left = subscription.getStartTickets %}
<table class="style">
  <thead>
    <tr>
      <th>Date</th>
      <th>Ticket</th>
      <th>Balance</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{ subscription.createdAt|club_datetime }}</td>
      <td>-</td>
      <td>{{ tickets_left }}</td>
    </tr>
    {% for log in subscription.subscriptionTicket %}
    {% set tickets_left = tickets_left-log.tickets %}
    <tr>
      <td>{{ log.createdAt|club_datetime }}</td>
      <td>{{ log.tickets }}</td>
      <td>{{ tickets_left }}</td>
    </tr>
    {% endfor %}
  </tbody>
  <tfoot>
    <tr>
      <th colspan="3">&#160;</th>
    </tr>
  </tfoot>
</table>



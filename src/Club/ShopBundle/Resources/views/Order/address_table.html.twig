<dl class="dl-horizontal">
            <dt>{% trans %}Company{% endtrans %}</dt>
            <dd>{{ address.companyName }}</dd>
            <dt>{% trans %}CVR{% endtrans %}</dt>
            <dd>{{ address.cvr }}</dd>
            <dt>{% trans %}Name{% endtrans %}</dt>
            <dd>{{ address.firstName }} {{ address.lastName }}</dd>
            <dt>{% trans %}Street{% endtrans %}</dt>
            <dd>{{ address.street }}</dd>
            <dt>{% trans %}Postal{% endtrans %}</dt>
            <dd>{{ address.postalCode }}</dd>
            <dt>{% trans %}City{% endtrans %}</dt>
            <dd>{{ address.city }}</dd>
            <dt>{% trans %}Country{% endtrans %}</dt>
            <dd>{{ address.country }}</dd>
</dl>

<fieldset>
  <legend>{% trans %}Period subscription{% endtrans %}</legend>

  {{ form_row(form.time_interval) }}
</fieldset>

<fieldset>
  <legend>{% trans %}Time range subscription{% endtrans %}</legend>

  {{ form_row(form.start_date) }}
  {{ form_row(form.expire_date) }}
</fieldset>

<fieldset>
  <legend>{% trans %}Other settings{% endtrans %}</legend>
  {{ form_row(form.ticket) }}
  {{ form_row(form.auto_renewal) }}
  {{ form_row(form.allowed_pauses) }}
  {{ form_row(form.booking) }}
  {{ form_row(form.team) }}
  {{ form_row(form.start_time) }}
  {{ form_row(form.stop_time) }}
  {{ form_row(form.location) }}

  {{ form_rest(form) }}
</fieldset>


{% for booking in bookings %}
BEGIN:VEVENT
DTSTART:{{ booking.firstDate|club_ical }}
DTEND:{{ booking.endDate|club_ical }}
{% if booking.guest %}
SUMMARY:{{ booking.user.name }} - {% trans %}Guest{% endtrans %}

{% else %}
SUMMARY:{{ booking.user.name }} - {% for u in booking.users %}{{ u.name }}{% endfor %}

{% endif %}
LOCATION:{{ booking.field.location.locationString }}
CATEGORIES:Booking
END:VEVENT
{% endfor %}
